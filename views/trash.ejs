<!doctype html>
<html>
<% include _header %>
<% include _menu %>
<body>
<table class="table table-hover">
    <thead>
    <tr>
        <th>#</th>
        <th>Email</th>
        <th>Message</th>
        <th>Time/Date</th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    <% messages.forEach(function(message){ %>

        <tr onclick="document.location = '/message';">

            <th scope="row"><%= message.id %></th>
            <td><%= message.email %></td>
            <td><%= message.title %></td>
            <td><%= message.createdAt %></td>
            <td>
                <button type="button" class="btn btn-sm btn-warning"><span class="glyphicon glyphicon-trash"
                                                                           aria-hidden="true"></span></button>
            </td>

        </tr>

    <% }); %>

    </tbody>
</table>
<% include _js_bootstap %>
</body>
</html>